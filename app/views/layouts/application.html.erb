<!DOCTYPE html>
<html>
<head>
  <title>Ask Angie</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav class="nav-bar">
    
    <% if user_signed_in? %>
     <%= link_to "Sign Out", signout_users_path, :method => :delete%>
    <% else %>
      <%= link_to "Log In", login_users_path, :method => :get %>
      <%= link_to "Sign Up", new_user_path %>
    <% end %>
  </nav> 
  
<div class="container">
<%= yield %>
</div>
</body>
</html>
